function! atplib#TempDir() "{{{1
    " Return temporary directory, unique for each user.
if has("python")
python << END
import vim
import tempfile
import os

USER=os.getenv("USER")
tmp=tempfile.mkdtemp(suffix="", prefix="atp_")
