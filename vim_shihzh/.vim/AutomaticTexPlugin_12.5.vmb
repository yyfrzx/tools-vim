" {{{ atplib#various#FormatLines
function! atplib#various#FormatLines()
    " This function is not using winsaveview() since this might change the
    " cursor position in the text.
    let cb = &clipboard
    let s = getpos("'<")
    let e = getpos("'>")
    set clipboard-=autoselect
    normal m`vipgq``
    call setpos("'<", s)
    call setpos("'>", e)
    let &clipboard=cb
endfunction
