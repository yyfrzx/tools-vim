function! <SID>BibYank(register, which)" {{{
    " Yank selection to register
    let g:register = a:register
    let bibkey =  get(b:ListOfBibKeys, a:which, 'no_key') 
    if bibkey == 'no_key'
	echomsg "[ATP:] Did you specify the entry?"
