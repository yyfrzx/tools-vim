function! atplib#compiler#ListErrorsFlags_A(A,L,P)
    " This has no o flag.
    let flags=['e', 'w', 'r', 'c', 'f', 'fi', 'F', 'h', 'p', 'P', 'all', 'All']
    if !g:atp_ParseLog
	call remove(flags, index(flags, 'p'))
    endif
    return join(flags, "\n")
endfunction
