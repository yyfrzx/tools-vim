	let @z=choice
    elseif a:register == '*'
	let @*=choice
    elseif a:register == '+'
	let @+=choice
    elseif a:register == '-'
	let @-=choice
    elseif a:register == '"'
	let @"=choice
    elseif a:register == ''
	if index(split(&cb, ','), 'unnamed') != -1
	    let @* = choice
	endif
	if index(split(&cb, ','), 'unnamedplus') != -1
	    let @+ = choice
